{
  "name": "paper-kit-react",
  "version": "1.2.0",
  "private": true,
  "homepage": "https://blinkz333.github.io/art_of_hongtae/",
  "dependencies": {
    "classnames": "2.2.6",
    "gh-pages": "^3.1.0",
    "moment": "2.26.0",
    "node-sass": "4.14.1",
    "nouislider": "14.5.0",
    "react": "16.13.1",
    "react-bootstrap-switch": "15.5.3",
    "react-coverflow": "^0.2.20",
    "react-datetime": "2.16.3",
    "react-device-detect": "^1.13.1",
    "react-dom": "16.13.1",
    "react-grid-gallery": "^0.5.5",
    "react-icons": "^3.11.0",
    "react-image-search-grid": "^0.10.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-slideshow-image": "^3.2.0",
    "react-typist": "^2.0.5",
    "reactstrap": "8.4.1",
    "typewriter-effect": "^2.13.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && gulp licenses",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/paper-kit.scss src/assets/css/paper-kit.css",
    "minify-sass": "node-sass src/assets/scss/paper-kit.scss src/assets/css/paper-kit.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/paper-kit.scss src/assets/css/paper-kit.css --source-map true",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "typescript": "3.9.5"
  },
  "devDependencies": {
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8"
  }
}
